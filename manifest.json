{
    "manifest_version": 3,
    "name": "API Call",
    "description": "Extension to make API calls",
    "version": "1.0",
    "action":{
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "128": "images/passport.png"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}
   
